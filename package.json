{
  "name": "checkerjs",
  "version": "1.0.8",
  "description": "Lightweight and easy to use data validation library for Node.js and the browser.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "test": "env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --require ts-node/register './src/test/**/*.ts'",
    "lint": "eslint  --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --ignore-path --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [
    "validation",
    "data",
    "data validation",
    "validation library",
    "data validation library",
    "node.js",
    "browser",
    "typescript",
    "javascript",
    "js",
    "ts",
    "react-validation"
  ],
  "author": "Kadir Osman Ust (https://github.com/KadoRaw)",
  "license": "MIT",
  "repository": "kadoraw/checkerjs",
  "homepage": "https://github.com/KadoRaw/checkerjs",
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
